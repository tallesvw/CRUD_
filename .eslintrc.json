{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript", "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"  
    }
}
